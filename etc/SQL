CREATE TABLE `do_si_gun` (
  `code` varchar(11) DEFAULT NULL,
  `name` varchar(50) DEFAULT NULL,
  `use_yn` varchar(1) DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;

CREATE TABLE `join_main` (
  `index` int(11) NOT NULL AUTO_INCREMENT,
  `join_name` varchar(50) DEFAULT NULL,
  `anyone_chk` varchar(1) DEFAULT NULL,
  `pwd` varchar(20) DEFAULT NULL,
  `join_area` varchar(300) DEFAULT NULL,
  `join_date` varchar(10) DEFAULT NULL,
  `gender` varchar(6) DEFAULT NULL,
  `hole_num` varchar(2) DEFAULT NULL,
  `teeup_time` varchar(1) DEFAULT NULL,
  `start_greenfee` varchar(9) DEFAULT NULL,
  `end_greenfee` varchar(9) DEFAULT NULL,
  `people_num` varchar(1) DEFAULT NULL,
  `join_exp` varchar(500) DEFAULT NULL,
  PRIMARY KEY (`index`)
) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;